{
    "name": "arcosashoring/arcosa",
    "description": "Connecting plugin for Craft projects",
    "type": "craft-plugin",
    "version": "1.0.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "arcosa"
    ],
    "support": {
        "docs": "https://github.com/arcosa/arcosa/blob/master/README.md",
        "issues": "https://github.com/arcosa/arcosa/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Walker",
            "homepage": "https://dustinwalker.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "arcosashoring\\arcosa\\": "src/"
        }
    },
    "extra": {
        "name": "Arcosa Shoring Craft Connector",
        "handle": "arcosa",
        "developer": "Dustin Walker",
        "developerUrl": "https://dustinwalker.com",
        "documentationUrl": "https://github.com/arcosa/arcosa/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/arcosa/arcosa/master/CHANGELOG.md",
        "components": {
            "arcosaService": "arcosashoring\\arcosa\\services\\ArcosaService"
        },
        "class": "arcosashoring\\arcosa\\Arcosa"
    }
}
